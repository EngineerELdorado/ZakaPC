{
  "index": "/index.html",
  "dataGroups": [{
    "name": "api-performance",
    "urls": [
      "/assets/i18n/**",
      "https://torapos-master.herokuapp.com/**"
    ],
    "cacheConfig": {
      "strategy": "performance",
      "maxSize": 10,
      "maxAge": "1d"
    }
  },
  // {
  //   "name": "api-freshness",
